{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-bcmath": "*",
        "ext-bz2": "*",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "friendsofsymfony/ckeditor-bundle": "^1.2",
        "friendsofsymfony/rest-bundle": "^2.4",
        "friendsofsymfony/user-bundle": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "hwi/oauth-bundle": "~0.6.3",
        "hybridauth/hybridauth": "~3.0",
        "jms/serializer-bundle": "~2.0",
        "liip/imagine-bundle": "^2.1",
        "nelmio/api-doc-bundle": "^3.3",
        "nelmio/cors-bundle": "^1.5",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.12",
        "sonata-project/admin-bundle": "^3.40",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "sonata-project/user-bundle": "^4.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^3.3",
        "symfony/debug-bundle": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/serializer": "^4.1.7",
        "symfony/framework-bundle": "^4.1.7",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "^4.1.7",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^3.3",
        "symfony/web-profiler-bundle": "^3.3",
        "symfony/web-server-bundle": "^4.1.7",
        "symfony/yaml": "^3.3",
        "vich/uploader-bundle": "^1.8"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "doctrine:schema:update --force": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BP7RMK8AZ70N4HQWRV810CQW",
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
